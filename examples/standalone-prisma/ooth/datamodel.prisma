type User {
  id: ID! @unique
  oothMeta: [OothMeta]! @relation(name: "OothMetasForUser")
}

type OothMeta {
  id: ID! @unique
  key: String!
  data: Json
  dataString: String  # for querying - JSON filtering is not available (https://github.com/prisma/prisma/issues/148)
  value: String
  user: User @relation(name: "OothMetasForUser")
}